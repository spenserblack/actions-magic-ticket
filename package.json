    "@actions/core": "^1.10.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.3",
    "@vercel/ncc": "^0.36.1",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "packageManager": "pnpm@8.6.2"
}
