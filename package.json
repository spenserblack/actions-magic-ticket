{
  "name": "actions-magic-ticket",
  "version": "0.0.0",
  "private": true,
  "description": "Congratulate a user when they get the magic issue/PR/discussion number",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "pnpm build && pnpm format && pnpm package && pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spenserblack/actions-magic-ticket.git"
  },
  "keywords": [
    "actions",
    "github",
    "issue"
  ],
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "nunjucks": "^3.2.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@octokit/webhooks-definitions": "^3.67.3",
    "@types/node": "^20.4.0",
    "@types/nunjucks": "^3.2.5",
    "@vercel/ncc": "^0.38.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "packageManager": "pnpm@8.6.2"
}
